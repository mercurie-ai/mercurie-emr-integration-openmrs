{
  "name": "@mercurie/emr-integration-openmrs",
  "version": "1.1.0",
  "description": "Middleware to integrate Mercurie with OpenMRS.",
  "main": "dist/server.js",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx --watch ./src/server.ts",
    "generate-types": "openapi-typescript ./node_modules/@mercurie/emr-integration/openapi.yaml --output ./src/emr-api-types.ts",
    "build": "npm run generate-types",
    "prestart": "npm run build",
    "predev": "npm run build"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@mercurie/emr-integration": "*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "openapi-typescript": "^7.9.1",
    "tsx": "^4.10.2"
  }
}